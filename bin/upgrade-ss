#!/usr/bin/env php
<?php

use SKYW\Upgrader\Cmds\{ Application, Command };

if(!class_exists(Command::class) || !class_exists(Application::class)){
  require_once __DIR__.'/../../vendor/autoload.php';
}



$app = new Application();
$command = new Command();

$allCMDs = $app->all();

foreach($allCMDs as  $cmd){
 $app->add($cmd);
}
$app->run();